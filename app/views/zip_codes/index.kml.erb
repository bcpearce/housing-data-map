<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <Style id="transBluePoly">
      <LineStyle>
        <width>1.5</width>
      </LineStyle>
      <PolyStyle>
        <color>18ff0000</color>
      </PolyStyle>
    </Style>
    <% @zip_codes.each do |z| %>
      <Placemark>
        <name><%= z.code %></name>
        <styleUrl>#transBluePoly</styleUrl>
        <%= z.kml.html_safe %>
      </Placemark>
    <% end %>
  </Document>
</kml>
