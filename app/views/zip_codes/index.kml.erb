<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <%= render 'kml_styles' %>
  <Document>
    <% @zip_codes.each do |z| %>
      <Placemark>
        <name><%= z.code %></name>
        <description>
          <%= z.latest_median_rent.rent.to_s unless z.latest_median_rent.nil? %>
        </description>
        <styleUrl><%= assign_style(z) %></styleUrl>
        <%= z.kml.html_safe %>
      </Placemark>
    <% end %>
  </Document>
</kml>
