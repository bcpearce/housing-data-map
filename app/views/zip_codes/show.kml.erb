<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <%= render 'kml_styles' %>
    <Placemark>
      <name><%= @zip_code.code %></name>
      <description>
        <%= @zip_code.latest_median_rent.print unless @zip_code.latest_median_rent.nil? %>
      </description>
      <styleUrl><%= assign_style(@zip_code) %></styleUrl>
      <%= @zip_code.kml.html_safe %>
    </Placemark>
  </Document>
</kml>
